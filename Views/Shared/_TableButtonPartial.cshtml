@model int

    <td style="width: 150px">
        <div class="btn-group" role="group">
            <a asp-action="Edit" asp-route-Id="@Model" class="btn btn-primary text-white">
                <i class="fas fa-edit"></i>
            </a>
            <a asp-action="Details" asp-route-Id="@Model" class="btn btn-success text-white">
                <i class="fas fa-list-alt"></i>
            </a>
            <a asp-action="Delete" asp-route-Id="@Model" class="btn btn-danger text-white">
                <i class="fas fa-trash-alt"></i>
            </a>
        </div>

    </td>